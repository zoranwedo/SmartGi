      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT c.ContractID,
       t.TourOpID,
       t.TourOp,
       t.BillingName,
       ct.Name,
       ct.Email1,
       c.HotelId,
       h.Hotel,
       h.Logo
FROM   [Contract] c
       INNER JOIN TourOp t
            ON  c.TourOpId = t.TourOpID
       INNER JOIN Hotel h
            ON  c.HotelId = h.HotelID
       LEFT OUTER JOIN TourOpContact tct
            ON  tct.TourOpId = t.TourOpID
       LEFT OUTER JOIN Contact ct
            ON  ct.ContactID = tct.ContactId
WHERE  c.ContractID = @ContractId
	AND (ct.ContactTypeId IS NULL 
		OR ct.ContactTypeId = 'CNT')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
