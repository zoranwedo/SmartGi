      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT r.ContractRateID,
	   r.ContractID,
	   r.CtrPeriodId,
	   r.Title,
	   r.Commision,
	   r.StateId,
	   r.HotelId,
	   r.TourOpId,
	   r.MarketId,
	   r.MealPlanId,
	   mp.MealPlan,
	   r.RoomTypeId,
	   clr.RoomType,
	   r.RateType,
	   typ.[Priority],
	   r.RateTypeId,
	   r.RateOption,
	   r.RateCodeId,
	   r.Created,
	   r.RateRecieved,
	   r.[Start],
	   r.Finish,
	   r.PerPax,
	   r.RatePax1,
	   r.RatePax2,
	   r.RatePax3,
	   r.RatePax4,
	   r.RatePax5,
	   r.RatePax6,
	   r.RatePax7,
	   r.RatePax8,
	   r.RatePax9,
	   r.RatePax10,
	   r.RateExtra,
	   r.ExtraPax,
	   r.LockChildrenRates,
	   r.RateChld1,
	   r.RateChld2,
	   r.RateChld3,
	   r.RateChld4,
	   r.Desctiption,
	   r.Allotment,
	   r.CutOff,
	   r.SpecialId,
	   r.SpecTitle,
	   r.BookingCode,
	   r.BkStart,
	   r.BkFinish,
	   CASE r.PerPax
			WHEN 0 THEN 'Per Room'
			ELSE 'Per Pax'
	   END                                AS PerPaxDisp,
	   CASE r.RateOption
			WHEN 0 THEN 'Per Arrival'
			WHEN 1 THEN 'Per Night'
			ELSE 'Inclusiv'
	   END                                AS RateOptionDisp,
	   c.Start                            AS CnStart,
	   c.Finish                           AS CnFinish,
	   clr.MaxOccupancy, 
	   clr.MinExtra,
	   clr.ChldCategories, 
	   clr.Chld1ShLabel, 
	   clr.Chld1From,
	   clr.Chld1Max, 
	   clr.Chld2ShLabel, 
	   clr.Chld2From, 
	   clr.Chld2Max,
	   clr.Chld3ShLabel, 
	   clr.Chld3From, 
	   clr.Chld3Max, 
	   clr.Chld4ShLabel,
	   clr.Chld4From, 
	   clr.Chld4Max, 
	   ot.OccText,
	   cp.Note
FROM   dbo.SelectContractRate(@UserName, @ContractID, NULL) r
	   INNER JOIN RateType typ
			ON  r.RateTypeId = typ.RateTypeID
	   INNER JOIN [Contract] c
			ON  r.ContractID = c.ContractID
	   INNER JOIN ContractPeriod cp
			ON  r.ContractID = cp.ContractId
			AND r.CtrPeriodId = cp.CtrPeriodID
	   INNER JOIN MealPlan mp
			ON mp.MealPlanId = r.MealPlanId
	   INNER JOIN dbo.ContractLayoutRoom(@ContractID) clr
			ON clr.ContractID = r.ContractID
			AND clr.HotelId = r.HotelId
			AND clr.RoomTypeId = r.RoomTypeId
	   LEFT OUTER JOIN dbo.ContractOccText(@ContractID) ot
			ON ot.ContractId = r.ContractID
			AND ot.HotelId = r.HotelId
			AND ot.RoomTypeId = r.RoomTypeId
WHERE r.SpecialId IS NULL </CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
