	LEFT OUTER JOIN TourOp  AS [to]
		ON  CONVERT(INT,fr.TourOpID) = [to].TourOpID
	LEFT OUTER JOIN vwResRoomngAndInvoice AS rr
		ON rr.FolioFull = fr.FolioFull
	LEFT OUTER JOIN PreInvoice AS p
		ON p.ResID = fr.ResID
	LEFT OUTER JOIN Hotel AS h
		ON p.HotelId = h.HotelID
