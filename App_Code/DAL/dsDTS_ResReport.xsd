                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	fr.ResID,
	fr.HotelHID,
	CONVERT(INT,fr.TourOpID) TourOpID,
	ISNULL([to].TourOp,fr.TourOpHID) TourOp,
	fr.Folio,
	fr.FolioOrd,
	fr.LastName,
	fr.[Name],
	fr.BkDate,
	fr.BkNumber,
	fr.ResGroup,
	fr.ResGroupName,
	CASE
		WHEN fr.[State] = 'EX' THEN 1
		ELSE 0
	END [Excluded], 
	CASE
		WHEN fr.ResType = 'C' THEN 'Cancelled'
		WHEN fr.ResType = 'S' THEN 'No Show'
		WHEN fr.TipoReg = 'H' THEN 'Check In'
		WHEN fr.TipoReg = 'O' THEN 'Check Out'
		ELSE 'Active'
	END [StateText], 
	fr.[State],
	fr.CheckIn,
	fr.CheckOut,
	fr.RoomTypeHID,
	fr.Nights,
	fr.Adult,
	fr.Junior,
	fr.Child,
	fr.Infant,
	fr.CompAdult, 
	fr.CompChild, 
	fr.CompJunior,
	fr.Created,
	fr.Modified,
	fr.FolioFull,
	rr.subNumber, 
	ISNULL(rr.genericName,fr.LastName) genericName,
	rr.personalGroupCode, 
	rr.[CHILD-TYPE],
	rr.[FREE],
	p.PreInvAmount,
	CASE 
		WHEN DENSE_RANK() OVER(PARTITION BY fr.ResID ORDER BY rr.subNumber) = 1 THEN p.PreInvAmount
		ELSE 0
	END Amount
FROM dbo.FilterReservation(
		@HotelHID, @TourOpHID, @TourOpId, @FolioFrom, @FolioTo, @Name,
		@BkDateFrom, @BkDateTo, @BkNumber, @ResGroup, @State, @ChkInFrom, @ChkInTo,
		@RoomTypeHID, @CreatedFrom, @CreatedTo, @ModifiedFrom, @ModifiedTo) AS fr
	LEFT OUTER JOIN TourOp  AS [to]
		ON  CONVERT(INT,fr.TourOpID) = [to].TourOpID
	LEFT OUTER JOIN ReservationRooming AS rr
		ON rr.ResID = fr.ResID
	LEFT OUTER JOIN PreInvoice AS p
		ON p.ResID = rr.ResID
WHERE ((@TourOpGroupID IS NULL) OR (fr.TourOpId IN (SELECT TourOpId FROM TourOpGroupMember WHERE TourOpGroupId = @TourOpGroupID)))
	AND ((@MarketID IS NULL) OR ([to].MarketId = @MarketID))
	AND ((@PayCondId IS NULL) OR ([to].PayCondId = @PayCondId))
       AND ((@OnlyGroup = 0) OR (fr.ResGroup IS NOT NULL))
ORDER BY
       fr.HotelHID,
       fr.TourOpHID,
       fr.TourOpID,
       fr.Folio,
       fr.FolioOrd,
       rr.subNumber</CommandText>
