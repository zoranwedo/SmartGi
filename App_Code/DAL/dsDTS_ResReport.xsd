	AND ((@MarketID IS NULL) OR ([to].MarketId = @MarketID))
	AND ((@PayCondId IS NULL) OR ([to].PayCondId = @PayCondId))
	AND ((@OnlyGroup = 0) OR (fr.ResGroup IS NOT NULL))
ORDER BY
       fr.HotelHID,
       h.SortPriority,
