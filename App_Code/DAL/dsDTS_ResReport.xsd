                    <CommandText>SELECT 
	fr.ResID,
	fr.HotelHID,
	h.HotelID, 
	h.Hotel,
	h.SortPriority,
	CONVERT(INT,fr.TourOpID) TourOpID,
	ISNULL([to].TourOp,fr.TourOpHID) TourOp,
	fr.Folio,
	fr.FolioOrd,
	fr.LastName,
	fr.[Name],
	fr.BkDate,
	fr.BkNumber,
	fr.ResGroup,
	fr.ResGroupName,
	CASE
		WHEN fr.[State] = 'EX' THEN 1
		ELSE 0
	END [Excluded], 
	CASE
		WHEN fr.ResType = 'C' THEN 'Canceled'
		WHEN fr.ResType = 'S' THEN 'No Show'
		WHEN fr.TipoReg = 'H' THEN 'Check In'
		WHEN fr.TipoReg = 'O' THEN 'Check Out'
		ELSE 'Active'
	END [StateText], 
	fr.[State],
	fr.CheckIn,
	fr.CheckOut,
	fr.RoomTypeHID,
	fr.Nights,
	fr.Adult,
	fr.Junior,
	fr.Child,
	fr.Infant,
	fr.CompAdult, 
	fr.CompChild, 
	fr.CompJunior,
	fr.Created,
	fr.Modified,
	fr.FolioFull,
	rr.number,
	rr.subNumber, 
	ISNULL(rr.genericName,fr.LastName) genericName,
	rr.personalGroupCode, 
	rr.[CHILD-TYPE],
	rr.[FREE],
	p.PreInvAmount,
	p.PreInvAmount Amount
FROM dbo.FilterReservation(
		@HotelHID, @TourOpHID, @TourOpId, @FolioFrom, @FolioTo, @Name,
		@BkDateFrom, @BkDateTo, @BkNumber, @ResGroup, @State, @ChkInFrom, @ChkInTo,
