                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	d.HotelId,
	h.Hotel,
	d.TourOpId,
	t.TourOp,
	d.DocDate,
	doc.AccountType,
	d.DocCode,
	d.DocNumber,
	d.Reference,
	d.Note,
	d.PayModeID,
	d.Amount Amount,
	d.BAmount BankAmount,
	ISNULL(d.Amount, 0) - ISNULL(d.BAmount, 0) NetAmount
FROM AccTransDocument AS d
	INNER JOIN AccDocument AS doc
		ON doc.TrCode = d.DocCode
	INNER JOIN Hotel AS h
		ON  h.HotelID = d.HotelId
	INNER JOIN TourOp AS t
		ON  t.TourOpID = d.TourOpId
WHERE d.Canceled = 0
	AND d.DocDate BETWEEN @DocDateFrom AND @DocDateTo
	AND (@DocTypeID IS NULL OR doc.DocTypeID = @DocTypeID)
</CommandText>
