          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="SpecialGroupDetailTableAdapter" GeneratorDataComponentClassName="SpecialGroupDetailTableAdapter" Name="SpecialGroupDetail" UserDataComponentName="SpecialGroupDetailTableAdapter">
