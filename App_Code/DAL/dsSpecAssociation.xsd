              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TOP 1
	gg.SpecGroupID, 
	gg.HotelId,
	h.Hotel,
	sg.Title,
	sg.BookingCode, 
	gg.gBkStart AS BkStart, 
	gg.gBkFinish AS BkFinish,
	gg.gStart, 
	gg.gFinish
FROM Special sg
	INNER JOIN SpecialInGroup sig
		ON sig.SpecialID = sg.SpecialID
	INNER JOIN Hotel h
		ON h.HotelId = sg.HotelID		
	INNER JOIN (
			SELECT g.SpecGroupID, s.HotelId, 
				MIN(s.[Start]) AS gStart, MAX(s.Finish) AS gFinish,
				MIN(s.BkStart) AS gBkStart, MAX(s.BkFinish) AS gBkFinish
			FROM Special s INNER JOIN SpecialInGroup g
				ON g.SpecialID = s.SpecialID
			WHERE g.SpecGroupID = @SpecGroupID
			GROUP BY g.SpecGroupID, s.HotelId) gg
		ON gg.SpecGroupID = sig.SpecGroupID
ORDER BY sg.[Start]
</CommandText>
