                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT c.ContractID,
       c.Template,
       c.HotelId,
       c.MarketId,
       c.Start,
       c.Finish,
       c.Commision,
       c.Title,
       c.Year,
       c.Description,
       c.StateId,
       c.CopyId,
       h.Hotel,
       crt.RateCodeId
FROM   [Contract] AS c
       INNER JOIN dbo.SelectUserHotel(@UserName) AS h
            ON  c.HotelId = h.HotelID
       INNER JOIN ContractRateType crt 
            ON c.ContractID = crt.ContractId
WHERE  (c.Template = 1) AND (c.SpecialID IS NULL)
	   AND ((@ContractID IS NULL) OR (c.ContractID = @ContractID))
	   AND ((@HotelId IS NULL) OR (c.HotelId = @HotelId))
	   AND ((@MarketId IS NULL) OR (c.MarketId = @MarketId))
	   AND ((@Start IS NULL) OR (c.Start &gt;= @Start))
	   AND ((@Finish IS NULL) OR (c.Finish &lt;= @Finish))
	   AND ((@Title IS NULL) OR (c.Title LIKE '%' + @Title + '%'))
	   AND ((@RateCodeID IS NULL) OR (crt.RateCodeId LIKE '%' + @RateCodeID + '%'))
ORDER BY
       h.SortPriority,
       c.Start,
       c.Title,
       c.ContractID

</CommandText>
