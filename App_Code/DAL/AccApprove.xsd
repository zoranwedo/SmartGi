                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT l.LogDate, l.UserName, l.LogMess, l.ObjEvent,
	CASE 
		WHEN l.LogMess LIKE '%REGISTER%' THEN 'registered'
		WHEN l.LogMess LIKE '%UNAPROVED%' THEN 'unapproved'
		WHEN l.LogMess LIKE '%APROVED%' THEN 'approved'
		WHEN l.LogMess LIKE '%modified%' THEN 'modified'
		WHEN l.LogMess LIKE '%UPDATE%' THEN 'edited'
		WHEN l.LogMess LIKE '%CANCEL%' THEN 'cancelled'
		ELSE l.LogMess
	END lMesage
FROM LogTable AS l
WHERE l.ObjType = 'AccDocument'
	AND l.ObjEventState = 'COMPLETED'
	AND l.ObjEvent &lt;&gt; 'Application'
	AND l.ObjId = @DocumentID
ORDER BY l.LogID DESC </CommandText>
