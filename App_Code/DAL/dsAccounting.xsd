            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT 
	CASE
		WHEN c.TranTypeID &lt;&gt; '01' THEN CONVERT(BIT,0) 
		WHEN d.Canceled = 1 THEN CONVERT(BIT,0)
		WHEN p.Closed = 1 THEN CONVERT(BIT,0)
		ELSE  CONVERT(BIT,1) 
	END AS CanModify
FROM AccTransDocument d 
	INNER JOIN AccDocument c
		ON c.TrCode = d.DocCode
	LEFT OUTER JOIN AccPeriod p
		ON d.HotelID = p.HotelID
		AND (DATEPART(YEAR,d.DocDate) = PeriodYear)
		AND (DATEPART(MONTH,d.DocDate) = PeriodMonth)
WHERE d.DocumentID = @DocumentID
</CommandText>
                <Parameters>
                  <Parameter AllowDbNull="false" AutogeneratedName="DocumentID" ColumnName="DocumentID" DataSourceName="LIVE_SMART_SQL.dbo.AccTransDocument" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@DocumentID" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="DocumentID" SourceColumnNullMapping="false" SourceVersion="Current" />
