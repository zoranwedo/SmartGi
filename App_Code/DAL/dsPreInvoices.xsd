              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT h.HotelID,
       h.Hotel,
       t.TourOpID,
       t.TourOp,
       i.BillingName,
       t.MarketId,
       t.PayCondId,
       i.InvoiceID,
       i.InvNumber,
       i.InvDate,
       i.InvDoc,
       i.InvType,
       p.FolioFull,
       p.ResGroup,
       p.BkNumber,
       s.BookingVoucher,
       s.DetailID,
       r.RoomTypeID,
       r.RoomType,
       s.DetText,
       b.BillingId,
       b.EnterDate,
       b.ExitDate,
       b.Nights,
       s.AdultNum,
       b.RateCodeId,
       b.Amount,
       b.HotelRate
FROM   Invoice                   AS i
       INNER JOIN InvStayDetail  AS s
            ON  i.InvoiceID = s.InvoiceId
       INNER JOIN InvBillDetail  AS b
            ON  s.InvoiceId = b.InvoiceId
            AND s.DetailID = b.DetailId
       INNER JOIN Hotel          AS h
            ON  i.HotelId = h.HotelID
       INNER JOIN HotelRoomType  AS r
            ON  h.HotelId = r.HotelId
            AND s.RoomTypeId = r.RoomTypeID
       INNER JOIN TourOp         AS t
            ON  i.TourOpId = t.TourOpID
       INNER JOIN PreInvoice     AS p
            ON  s.PreInvId = p.PreInvID
WHERE  (i.InvDoc = 'PRO')
       AND (b.HotelRate = 0)
       AND ((@HotelID IS NULL) OR (h.HotelID = @HotelID))
       AND ((@TourOpID IS NULL) OR (t.TourOpID = @TourOpID))
       AND ((@InvType IS NULL) OR (i.InvType = @InvType))
       AND (i.CheckIn BETWEEN ISNULL(@CkInStart,i.CheckIn) AND ISNULL(@CkInFinish,i.CheckIn))
       AND (i.CheckOut BETWEEN ISNULL(@CkOutStart,i.CheckOut) AND ISNULL(@CkOutFinish,i.CheckOut))
       AND (i.InvNumber BETWEEN ISNULL(@InvNumStart,i.InvNumber) AND ISNULL(@InvNumFinish,i.InvNumber))
       AND ((@ResGroup IS NULL) OR (p.ResGroup LIKE '%' + @ResGroup + '%'))
       AND ((@BkNumber IS NULL) OR (p.BkNumber LIKE '%' + @BkNumber + '%'))
--ORDER BY p.FolioFull, b.EnterDate, b.ExitDate, i.InvNumber
ORDER BY i.InvNumber, p.FolioFull</CommandText>
