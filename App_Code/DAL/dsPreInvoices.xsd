          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="GroupProformaTableAdapter" GeneratorDataComponentClassName="GroupProformaTableAdapter" Name="GroupProforma" UserDataComponentName="GroupProformaTableAdapter">
