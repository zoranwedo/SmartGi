       ipg.AmountC1,
       ipg.AmountC2,
       ipg.AmountC3,
       ipg.AmountC4,
       ipg.DetailAmount,
       ipg.BookingDate,
       TourOp.Address,
       TourOp.City,
       TourOp.State      AS Expr1,
       TourOp.ZipCode,
       TourOp.Country,
       TourOp.Tel1,
       ipg.CCIReview,
       cc.Company  AS CCIHotel,
       cc.RNC      AS CCIRNC,
       cc.Address  AS CCIAddress,
       cc.City     AS CCICity,
       cc.ZipCode  AS CCIZipCode,
       cc.Country  AS CCICountry,
       cc.State    AS CCIState,
       cc.Tel      AS CCITel,
       cc.Fax      AS CCIFax,
       TourOp.MarketId, 
       TourOp.PayCondId
FROM   dbo.Invoice_Print_Group(
           @HotelID,
           @TourOpId,
           @HRate,
           @InvNumFrom,
           @InvNumTo,
           @ChkInFrom,
           @ChkInTo,
           @InvDateFrom,
           @InvDateTo,
           @InvDoc,
           @InvType,
           @State,
           @IncludeCancel,
           @CCIReview
       )                 AS ipg
       LEFT OUTER JOIN TourOp
            ON  ipg.TourOpId = TourOp.TourOpID
       LEFT OUTER JOIN Market m
            ON m.MarketID = TourOp.MarketId
       INNER JOIN Company  AS cc
            ON cc.[Local] = m.[Local]
WHERE  ((@MarketId IS NULL) OR (TourOp.MarketId = @MarketId))
   AND ((@PayCondId IS NULL) OR (TourOp.PayCondId = @PayCondId))
ORDER BY
       ipg.InvNumber,
       ipg.CheckIn</CommandText>
