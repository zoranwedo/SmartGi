            <SelectCommand>
              <DbCommand CommandType="Text" ModifiedByUser="true">
                <CommandText>SELECT i.InvDoc + '_' + 
	ISNULL(UPPER(sh.HotelEHID),UPPER(h.HotelHID)) + '#' + 
	CONVERT(VARCHAR(20), i.InvNumber) + '(' + 
	CONVERT(VARCHAR(20),i.InvoiceID) + ')' 
FROM Invoice i
	INNER JOIN Hotel h
		ON h.HotelID = i.HotelId
	LEFT OUTER JOIN smartHotel sh
		ON sh.HotelId = h.HotelId
WHERE i.InvoiceID = @InvoiceID
</CommandText>
