                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT p.InvoiceID,
       p.InvNumber,
       p.InvOrder,
       p.HotelId,
       p.Hotel,
       p.TourOpId,
       p.BillingName,
       p.TourOp,
       p.InvDoc,
       p.InvType,
       p.ResGroup,
       p.InvDate,
       p.CheckIn,
       p.CheckOut,
       p.Amount,
       p.RefText,
       p.InvRefText,
       p.InvPrnText,
       p.State,
       p.InvoiceState,
       h.RNC,
       h.Address,
       h.City,
       h.Tel,
       h.Fax,
       h.State       AS Expr1,
       t.Address    AS AddressTO,
       t.City       AS CityTO,
       t.State      AS StateTO,
       t.Country    AS CountryTO,
       t.ZipCode    AS ZipCodeTO,
       cc.Company   AS CCIHotel,
       cc.RNC      AS CCIRNC,
       cc.Address  AS CCIAddress,
       cc.City     AS CCICity,
       cc.ZipCode  AS CCIZipCode,
       cc.Country  AS CCICountry,
       cc.State    AS CCIState,
       cc.Tel      AS CCITel,
       cc.Fax      AS CCIFax,
       c.Company   AS HotelBillingTo
FROM   dbo.Invoice_Print(@InvoiceID, @HRate) AS p
       INNER JOIN Hotel h
            ON  p.HotelId = h.HotelID
       LEFT OUTER JOIN TourOp t
            ON  p.TourOpId = t.TourOpID
       LEFT OUTER JOIN Company2Bill(@InvoiceID, @HRate) c
            ON h.BillingTo = c.CompanyID
       CROSS JOIN Company  AS cc
WHERE cc.CompanyID = 1
</CommandText>
