          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="ContractRateTableAdapter" GeneratorDataComponentClassName="ContractRateTableAdapter" Name="ContractRate" UserDataComponentName="ContractRateTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	r.ContractRateID, 
	r.ContractId, r.HotelId, 
	r.RateCodeId, r.MealPlanId, 
	r.RoomTypeId, o.RoomType, 
	r.CtrPeriodId, r.Start, r.Finish, 
	r.PerPax, r.RateOption, 
    CASE WHEN r.RateExtra&gt;1 THEN r.RatePax1 ELSE NULL END AS RatePax1, 
    CASE WHEN r.RateExtra&gt;2 THEN r.RatePax2 ELSE NULL END AS RatePax2, 
    CASE WHEN r.RateExtra&gt;3 THEN r.RatePax3 ELSE NULL END AS RatePax3, 
    CASE WHEN r.RateExtra&gt;4 THEN r.RatePax4 ELSE NULL END AS RatePax4, 
    CASE WHEN r.RateExtra&gt;5 THEN r.RatePax5 ELSE NULL END AS RatePax5, 
    CASE WHEN r.RateExtra&gt;6 THEN r.RatePax6 ELSE NULL END AS RatePax6, 
    CASE WHEN r.RateExtra&gt;7 THEN r.RatePax7 ELSE NULL END AS RatePax7, 
    CASE WHEN r.RateExtra&gt;8 THEN r.RatePax8 ELSE NULL END AS RatePax8, 
    CASE WHEN r.RateExtra&gt;9 THEN r.RatePax9 ELSE NULL END AS RatePax9, 
    r.RatePax10, 
    r.RateChld1, r.RateChld2, r.RateChld3, r.RateChld4, 
    r.RateExtra, 
	CASE r.RateExtra
		WHEN 1 THEN r.RatePax1
		WHEN 2 THEN r.RatePax2 
		WHEN 3 THEN r.RatePax3
		WHEN 4 THEN r.RatePax4
		WHEN 5 THEN r.RatePax5
		WHEN 6 THEN r.RatePax6
		WHEN 7 THEN r.RatePax7
		WHEN 8 THEN r.RatePax8
		WHEN 9 THEN r.RatePax9
		ELSE  r.RatePax10
	END AS RatePaxEx, 
    r.Allotment, r.CutOff, 
    o.Category, o.MaxROccupancy, 
    r.Desctiption,
    r.ImpState, r.Approved
FROM 
	ContractRate AS r 
	INNER JOIN ContractMaxRoomOccupamcy AS o 
		ON r.ContractId = o.ContractId 
	   AND r.HotelId = o.HotelId 
	   AND r.RoomTypeId = o.RoomTypeId
WHERE (r.ContractId = @ContractId)
	AND (r.SpecialId IS NULL)
ORDER BY r.RoomTypeId, o.Category, r.RateCodeId, r.MealPlanId, r.Start</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="ContractId" ColumnName="ContractId" DataSourceName="TEST_SMART_SQL.dbo.ContractRate" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@ContractId" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="ContractId" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
