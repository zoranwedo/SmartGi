          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="FilterContractRate1TableAdapter" GeneratorDataComponentClassName="FilterContractRate1TableAdapter" Name="FilterContractRate1" UserDataComponentName="FilterContractRate1TableAdapter">
            <MainSource>
              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT r.ContractRateID,
       r.ContractID,
       r.CtrPeriodId,
       r.Title,
       r.Commision,
       r.StateId,
       r.HotelId,
       r.TourOpId,
       r.MarketId,
       r.MealPlanId,
       r.RoomTypeId,
       r.RateType,
       r.RateTypeId,
       r.RateOption,
       r.RateCodeId,
       r.Created,
       r.RateRecieved,
       r.Start,
       r.Finish,
       r.PerPax,
       r.RatePax1,
       r.RatePax2,
       r.RatePax3,
       r.RatePax4,
       r.RatePax5,
       r.RatePax6,
       r.RatePax7,
       r.RatePax8,
       r.RatePax9,
       r.RatePax10,
       r.RateExtra,
       r.LockChildrenRates,
       r.RateChld1,
       r.RateChld2,
       r.RateChld3,
       r.RateChld4,
       CASE WHEN r.SpecialId IS NULL 
		THEN c.[Description] 
		ELSE s.Description 
       END								  AS Desctiption,
       r.Allotment,
       r.CutOff,
       r.SpecialId,
       r.SpecTitle,
       r.BookingCode,
       r.BkStart,
       r.BkFinish,
       h.Hotel,
       t.TourOp,
       CASE r.PerPax
            WHEN 0 THEN 'Per Room'
            ELSE 'Per Pax'
       END                                AS PerPaxDisp,
       CASE r.RateOption
            WHEN 0 THEN 'Per Arrival'
            WHEN 1 THEN 'Per Night'
            ELSE 'Inclusiv'
       END                                AS RateOptionDisp,
       c.Start                            AS CnStart,
       c.Finish                           AS CnFinish,
       occ.Chld1ShLabel,
       occ.Chld1AgShLabel,
       occ.Chld2ShLabel,
       occ.Chld2AgShLabel,
       occ.Chld3ShLabel,
       occ.Chld3AgShLabel,
       occ.Chld4ShLabel,
       occ.Chld4AgShLabel,
       typ.Priority,
       cp.Note,
       sd.Discounts,
       r.FRSpecialID,
       r.ExtraPax,
       ISNULL(cRoom.HotelSectionId, 0)    AS HotelSectionId,
       cRoom.Category,
       st.SpecialTermsText
FROM   dbo.FilterContractRate(
           @UserName,
           @HotelId,
           @TourOpId,
           @MarketId,
           @Title,
           @Created,
           @RateRecieved,
           @MealPlanID,
           @StateId,
           @Commisionable,
           @RateType,
           @RateTypeId,
           @RoomTypeId,
           @Start,
           @Finish,
           @BkStart,
           @BkFinish,
           @RateOption,
           @RateCodeID
       )                                  AS r
       INNER JOIN Hotel                   AS h
            ON  r.HotelId = h.HotelID
       INNER JOIN TourOp                  AS t
            ON  r.TourOpId = t.TourOpID
       INNER JOIN RateType                AS typ
            ON  r.RateTypeId = typ.RateTypeID
       INNER JOIN HotelRoomType           AS cRoom
            ON  r.HotelId = cRoom.HotelId
            AND r.RoomTypeId = cRoom.RoomTypeID
       LEFT OUTER JOIN Special            AS s
            ON  r.SpecialId = s.SpecialID
       LEFT OUTER JOIN SpecialTerms       AS st
            ON  r.SpecialId = st.SpecialID
       LEFT OUTER JOIN dbo.SpecialDiscounts() AS sd
            ON  r.SpecialId = sd.SpecialID
       LEFT OUTER JOIN CONTRACT           AS c
            ON  r.ContractID = c.ContractID
       LEFT OUTER JOIN ContractOccupancy  AS occ
            ON  r.ContractID = occ.ContractID
       LEFT OUTER JOIN ContractPeriod     AS cp
            ON  r.ContractID = cp.ContractId
            AND r.CtrPeriodId = cp.CtrPeriodID
ORDER BY
       h.Hotel,
       t.TourOp,
       r.Start,
       typ.Priority DESC
</CommandText>
