              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectName="LIVE_SMART_SQL.dbo.FilterContracts" DbObjectType="Function" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	fc.ContractID, 
	fc.SpecialID, 
	fc.HotelId, 
	fc.Hotel, 
	fc.TourOpId, 
	fc.TourOp,
	fc.MarketId, 
	fc.Title, 
	fc.RateType, 
	fc.RateTypeId, 
	fc.RateCodeId,
	fc.[Start], 
	fc.Finish, 
	fc.[Description], 
	fc.BookingCode, 
	fc.BkStart,
	fc.BkFinish, 
	fc.Discount, 
	fc.RateRecieved, 
	fc.Created, 
	fc.CreatedBy,
	fc.[State], 
	fc.StateId, 
	fc.[Active],
	COALESCE(
		cc.SihotRateType, 
		spo.SihotRateType, 
		sup.SihotRateType
	) SihotRateType,
	COALESCE(
		cc.RateComment,
		spo.RateComment,
		sup.RateComment
	) RateComment,
	fc.DiscountValue
FROM dbo.FilterContracts(
			@UserName, 
			@HotelId, 
			@TourOpId, 
			@MarketId, 
			@Title,
			@RateType, 
			@RateTypeId, 
			@RateCodeId, 
			@Start, 
			@Finish, 
			@BkStart, 
			@BkFinish,
			@RateRecieved, 
			@InclInactive
		) fc
	LEFT OUTER JOIN ContractComment cc
		ON cc.ContractID = fc.ContractID
		AND fc.RateType = 3
	LEFT OUTER JOIN SpecialComment spo
		ON spo.SpecialID = fc.SpecialID
		AND fc.RateType = 1
	LEFT OUTER JOIN SupplementComment sup
		ON sup.SupplementID = fc.SpecialID
		AND fc.RateType = 9</CommandText>
