              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectName="LIVE_SMART_SQL.dbo.SelectUserTourOp" DbObjectType="Function" FillMethodModifier="Public" FillMethodName="FillPlain" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetDataPlain" GeneratorSourceName="FillPlain" GetMethodModifier="Public" GetMethodName="GetDataPlain" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataPlain" UserSourceName="FillPlain">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    uto.TourOpID, 
    uto.MarketId, 
    uto.ParentTourOpId, 
    uto.TourOp, 
    uto.BillingName, 
    uto.RNC, 
    uto.Address, 
    uto.City, 
    uto.State, 
    uto.ZipCode, 
    uto.Country, 
    uto.Tel1, 
    uto.Tel2, 
    uto.Tel3, 
    uto.Fax, 
    uto.URL, 
    uto.Email1, 
    uto.Email2, 
    uto.PayModeId, 
    uto.PayCondId, 
    uto.PayCondExtId, 
    uto.CreditDays, 
    uto.BillingParent, 
    uto.Active, 
    uto.Note
FROM dbo.SelectUserTourOp(@UserName) AS uto
WHERE (@ActiveOnly IS NULL OR uto.Active = @ActiveOnly) AND 	
    (@MarketId IS NULL OR uto.MarketId = @MarketId) AND 
    (@PayCondId IS NULL OR uto.PayCondId = @PayCondId) AND 
    (@PayModeId IS NULL OR uto.PayModeId = @PayModeId) AND 
    (@CreditDays IS NULL OR uto.CreditDays = @CreditDays) AND 
    (@TourOp IS NULL OR uto.TourOp LIKE '%' + @TourOp + '%') AND 
    (@City IS NULL OR uto.City LIKE '%' + @City + '%') AND 
    (@Country IS NULL OR uto.Country LIKE '%' + @Country + '%') 
</CommandText>
