                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT Invoice.InvoiceID,
       Invoice.InvNumber,
       Invoice.InvOrder,
       Invoice.InvDate,
       Invoice.InvDoc,
       Invoice.InvType,
       Invoice.ResGroup,
       Invoice.HotelId,
       Invoice.TourOpId,
       Invoice.BillingName,
       Invoice.CheckIn,
       Invoice.CheckOut,
       Invoice.Nights,
       Invoice.Amount,
       Invoice.HAmount,
       Invoice.ITBIS,
       Invoice.InvRefText,
       Invoice.InvPrnText,
       Invoice.InvComment,
       Invoice.State,
       Invoice.CreateUser,
       Invoice.CreateDate,
       Invoice.EditUser,
       Invoice.EditDate,
       Hotel.Hotel,
       TourOp.TourOp,
       CASE Invoice.InvType
            WHEN 0 THEN 'Stay Inv'
            WHEN 1 THEN 'Group Inv'
            WHEN 2 THEN 'Cancel Inv'
            ELSE 'No-Show Inv'
       END AS InvTypeDisp,
       Invoice.DeliveryDate,
       Invoice.HOAmount,
       InvoiceState.InvoiceState,
       Invoice.CCIReview,
       Invoice.ReviewDate,
       InvAccBalance.DocumentID, 
       InvAccBalance.DocCode, 
       InvAccBalance.DocNumber,
       InvAccBalance.BalanceDate, 
       InvAccBalance.Balance, 
       InvAccBalance.AppAmount,
       dbo.IsAccPeriodClosed(NULL, Invoice.HotelId, 'FT-RT', Invoice.InvDate) AccPeriod
FROM   Invoice
       INNER JOIN Hotel
            ON  Invoice.HotelId = Hotel.HotelID
       INNER JOIN TourOp
            ON  Invoice.TourOpId = TourOp.TourOpID
       INNER JOIN InvoiceState
            ON  Invoice.State = InvoiceState.InvoiceStateID
       LEFT OUTER JOIN InvAccBalance 
            ON  Invoice.InvoiceID = InvAccBalance.InvoiceID
WHERE  (Invoice.InvoiceID = @InvoiceID)
</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="InvoiceID" ColumnName="InvoiceID" DataSourceName="LIVE_SMART_SQL.dbo.Invoice" DataTypeServer="int" DbType="Int32" Direction="Input" ParameterName="@InvoiceID" Precision="0" ProviderType="Int" Scale="0" Size="4" SourceColumn="InvoiceID" SourceColumnNullMapping="false" SourceVersion="Current" />
