            <Sources>
              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="true" GeneratorGetMethodName="GetNewDataByID" GetMethodModifier="Public" GetMethodName="GetNewDataByID" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetNewDataByID" UserSourceName="GetNewDataByID">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT TOP 1 
	i.InvoiceId,
	0				AS DetailID,
	1				AS DetOrder,
	rtrim(r.RoomTypeID)	AS RoomTypeId,
	rtrim(m.MealPlanID)	AS MealPlanId,	
	1				AS RoomsNum,
	0				AS Amount,
	d.AdultNum,
	d.BookingDate,
	d.BookingVoucher,
	d.Chld1Num,
	d.Chld2Num,
	d.Chld3Num,
	d.Chld4Num,
	d.DetRefText,
	d.DetText,
	d.FolioFull,
	d.HAmount,
	d.PayAmount,
	d.Payed,
	d.PaymentID,
	d.PreAmount,
	d.PreDetId,
	d.PreInvId,
	d.StateID,
	i.HotelId,
	d.CompAdultNum,
	d.CompChildNum,
	d.CompJuniorNum
FROM Invoice i
	INNER JOIN HotelMealPlan m 
		ON m.HotelID = i.HotelId
	INNER JOIN HotelRoomType r 
		ON r.HotelId = i.HotelId
	LEFT OUTER JOIN InvStayDetail d
		ON d.InvoiceID = i.InvoiceId
		AND d.DetailID = 0
WHERE (i.InvoiceId = @InvoiceId)
</CommandText>
