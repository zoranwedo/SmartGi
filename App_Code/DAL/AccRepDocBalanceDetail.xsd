                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT HotelId, TourOpId, DocumentID, Hotel, TourOp, MarketId, CreditDays, 
	TrDocClass, TrClass, TrAccount, DocCode, DocNumber, DocOrd, DocDate, 
	DebitAmount, CreditAmount, DebitHAmount, CreditHAmount, Balance, HBalance,
	Reference, Note, DepID, EmpID, DepName, EmpName
FROM dbo.AccRepDocBalanceSign(@HotelID, @HotelGroupID, @TourOpID, @TourOpGroupID, 
	@MarketID, @PayCondID, @AsOffDate, @DocFrom, @TrDocClass, @TrClass, 
	@TrCode, @AccTypeID, @TranTypeID, @DocTypeID, @TrSign, @DepID, @EmpID) AS b
WHERE   
	(@RptOption = 0) OR
	(@RptOption=1 AND Balance&lt;&gt;COALESCE(DebitAmount,CreditAmount) AND Balance&lt;&gt;0) OR
	(@RptOption=2 AND Balance=0) OR
	(@RptOption=3 AND Balance=COALESCE(DebitAmount,CreditAmount)) OR
	(@RptOption=4 AND Balance&lt;&gt;0)
ORDER BY Hotel, TourOp, TrDocClass, TrClass, DocCode</CommandText>
