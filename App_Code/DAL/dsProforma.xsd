            <MainSource>
              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT d.InvDocID, d.GroupName, d.TourOpID, t.TourOp, d.CheckInFrom, d.CheckInTo,
       d.CheckOutFrom, d.CheckOutTo, d.InvDocNum, d.InvDocState,
       CASE d.InvDocState
              WHEN 0 THEN 'Created'
              WHEN 1 THEN 'Updated'
              WHEN 2 THEN 'Definitive'
              ELSE 'Unknown'
       END InvDocStateDesc
FROM InvDocument d
	INNER JOIN InvDocumentHotel h 
		ON h.InvDocID = d.InvDocID
	INNER JOIN TourOp t
		ON t.TourOpID = d.TourOpID
	LEFT OUTER JOIN vwInvResDocGroupName gn
		ON gn.InvDocID = d.InvDocID
WHERE d.InvResType = 1
	AND (@GroupName IS NULL OR d.GroupName LIKE '%' + @GroupName + '%' OR gn.ResGroupName LIKE '%' + @GroupName + '%')
	AND (@TourOpID IS NULL OR d.TourOpID = @TourOpID)
	AND (@HotelID IS NULL OR h.HotelID = @HotelID)
	AND ((@State IS NULL)
		OR (d.InvDocState = @State) 
		OR (@State = 3 AND d.InvDocState &lt; 2))
