	CASE b.RateType 
		WHEN 9 THEN 1 
		ELSE 0 
	END				DetOrder, 
	d.GuestName			DetText,
	d.BkNumber			BookingVoucher, 
	d.BkDate				BookingDate,
	b.Arrival				EnterDate, 
