                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT 
	d.InvDocID, 
	d.GroupName, 
	d.TourOpID, 
	t.TourOp, 
	d.CheckInFrom, 
	d.CheckInTo,
	d.CheckOutFrom, 
	d.CheckOutTo, 
	d.InvDocNum, 
	d.InvDocState,
	CASE d.InvDocState
		WHEN 0 THEN 'Created'
		WHEN 1 THEN 'Updated'
		WHEN 2 THEN 'Definitive'
		ELSE 'Unknown'
	END InvDocStateDesc
FROM InvDocument d
	INNER JOIN InvDocumentHotel h 
		ON h.InvDocID = d.InvDocID
	INNER JOIN TourOp t
		ON t.TourOpID = d.TourOpID
WHERE d.InvResType = 1
	AND (@HotelID IS NULL OR h.HotelID = @HotelID)
	AND (@HotelGroupID IS NULL OR (h.HotelId IN (SELECT HotelId FROM HotelGroupMember WHERE HotelGroupId = @HotelGroupID)))
	AND (@TourOpID IS NULL OR d.TourOpID = @TourOpID)
	AND (@TourOpGroupID IS NULL OR (d.TourOpId IN (SELECT TourOpId FROM TourOpGroupMember WHERE TourOpGroupId = @TourOpGroupID)))
	AND (@MarketID IS NULL OR t.MarketId = @MarketID)
	AND (@PayCondID IS NULL OR t.PayCondId = @PayCondID)
	AND ((@State IS NULL)
		OR (d.InvDocState = @State) 
		OR (@State = 3 AND d.InvDocState &lt; 2))
ORDER BY d.InvDocNum</CommandText>
