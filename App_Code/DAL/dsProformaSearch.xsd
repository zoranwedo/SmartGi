              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	i.InvDocID InvoiceID,
	s.BookingID DetailID,
	b.InvResID,
	CASE b.RateType 
		WHEN 9 THEN 1 
		ELSE 0 
	END DetOrder, 
	s.GuestName DetText,
	b.BkDate BookingDate,
	b.BkNumber BookingVoucher,
	b.RoomTypeId,
	b.MealPlanId,
	s.Rooms RoomsNum,
	b.Adult AdultNum,
	b.Infant Chld1Num,
	b.Child Chld2Num, 
	b.Junior1 Chld3Num, 
	b.Junior2 Chld4Num,
	b.CompAdult	CompAdultNum, 
	b.CompChild	CompChildNum,
	b.CompJunior CompJuniorNum,
	b.FolioFull FolioFull,
	b.Arrival EnterDate, 
	b.Departure ExitDate, 
	b.TravelNights Nights, 
	b.ChargedNights ChargeNights, 
	b.RateCodeID RateCodeId, 
	b.RateType RateType,
	CONVERT(BIT,0) HotelRate,
	b.PaxAdult Pax1, 0 Pax2, 0 Pax3, 0 Pax4, 0 Pax5, 
	0 Pax6, 0 Pax7, 0 Pax8, 0 Pax9, 0 Pax10, 
	0 Extra, b.PaxChld1 Chld1, 
	b.PaxChld2 Chld2, b.PaxChld3 Chld3, b.PaxChld4 Chld4, 
	b.RateAdult RatePax1, CONVERT(MONEY,0) RatePax2, 
	CONVERT(MONEY,0) RatePax3, CONVERT(MONEY,0) RatePax4, 
	CONVERT(MONEY,0) RatePax5, CONVERT(MONEY,0) RatePax6, 
	CONVERT(MONEY,0) RatePax7, CONVERT(MONEY,0) RatePax8, 
	CONVERT(MONEY,0) RatePax9, CONVERT(MONEY,0) RatePax10, 
	b.RateExtra	RatePaxEx, b.RateChld1 RateChld1, 
	b.RateChld2 RateChld2, b.RateChld3 RateChld3, b.RateChld4 RateChld4, 
	b.Amount, 
	r.BookingCode,
	r.PerPax, r.PerDay, sp.IsPercent,
	cp.Chld1Label cChld1Label, cp.Chld2Label cChld2Label, 
	cp.Chld3Label cChld3Label, cp.Chld4Label Chld4Label,
	hp.Chld1Label hChld1Label, hp.Chld2Label hChld2Label, 
	hp.Chld3Label hChld3Label, hp.Chld4Label hChld4Label, 
	CONVERT(DECIMAL(18,2),0) HotelPercent,
	CASE
		-- Complimentary reservation
		WHEN b.RateType = 0 THEN 2
		-- Has Complimetary pax		
		WHEN ISNULL(b.CompAdult,0) + ISNULL(b.CompChild,0) &gt; 0 THEN 1
		-- No Complimentary pax 
		ELSE 0
	END Complimentary
FROM InvDocument AS i
	INNER JOIN InvDocumentItem AS ii
		ON ii.InvDocID = i.InvDocID
	INNER JOIN InvResBooking AS s
		ON s.BookingID = ii.BookingID
	INNER JOIN InvResDetail AS b
		ON b.BookingID = s.BookingID
	LEFT OUTER JOIN Rate r
		ON r.RateID = b.RateID
	LEFT OUTER JOIN Supplement sp
		ON sp.SupplementID = r.SupplementId
	LEFT OUTER JOIN ContractPolicy cp
		ON cp.ContractId = r.ContractId
	LEFT OUTER JOIN HotelPolicy hp
		ON hp.HotelID = s.HotelId
WHERE i.InvDocID = @InvoiceID
	AND (@Cancelled = 1 OR s.Penalty = 1 OR s.InHouseState &lt;&gt; -2)</CommandText>
