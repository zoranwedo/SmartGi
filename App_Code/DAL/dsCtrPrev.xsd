                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
	r.ContractRateID, 
	r.ContractId, r.HotelId, 
	r.RateCodeId, r.MealPlanId, 
	r.RoomTypeId, o.RoomType, 
	r.CtrPeriodId, r.Start, r.Finish, 
	r.PerPax, r.RateOption, 
    CASE WHEN r.RateExtra&gt;1 THEN r.RatePax1 ELSE NULL END AS RatePax1, 
    CASE WHEN r.RateExtra&gt;2 THEN r.RatePax2 ELSE NULL END AS RatePax2, 
    CASE WHEN r.RateExtra&gt;3 THEN r.RatePax3 ELSE NULL END AS RatePax3, 
    CASE WHEN r.RateExtra&gt;4 THEN r.RatePax4 ELSE NULL END AS RatePax4, 
    CASE WHEN r.RateExtra&gt;5 THEN r.RatePax5 ELSE NULL END AS RatePax5, 
    CASE WHEN r.RateExtra&gt;6 THEN r.RatePax6 ELSE NULL END AS RatePax6, 
    CASE WHEN r.RateExtra&gt;7 THEN r.RatePax7 ELSE NULL END AS RatePax7, 
    CASE WHEN r.RateExtra&gt;8 THEN r.RatePax8 ELSE NULL END AS RatePax8, 
    CASE WHEN r.RateExtra&gt;9 THEN r.RatePax9 ELSE NULL END AS RatePax9, 
    r.RatePax10, 
    r.RateChld1, r.RateChld2, r.RateChld3, r.RateChld4, 
    r.RateExtra, 
	CASE r.RateExtra
		WHEN 1 THEN r.RatePax1
		WHEN 2 THEN r.RatePax2 
		WHEN 3 THEN r.RatePax3
		WHEN 4 THEN r.RatePax4
		WHEN 5 THEN r.RatePax5
		WHEN 6 THEN r.RatePax6
		WHEN 7 THEN r.RatePax7
		WHEN 8 THEN r.RatePax8
		WHEN 9 THEN r.RatePax9
		ELSE  r.RatePax10
	END AS RatePaxEx, 
    r.Allotment, r.CutOff, 
    o.Category, o.MaxROccupancy, 
    r.Desctiption
FROM 
	ContractRate AS r 
	INNER JOIN ContractMaxRoomOccupamcy AS o 
		ON r.ContractId = o.ContractId 
	   AND r.HotelId = o.HotelId 
	   AND r.RoomTypeId = o.RoomTypeId
WHERE (r.ContractId = @ContractId)
ORDER BY r.RoomTypeId, o.Category, r.RateCodeId, r.MealPlanId, r.Start</CommandText>
