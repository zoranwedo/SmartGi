                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT q.TrDocID, q.TrCode, q.AccountType
FROM (
	SELECT d.TrDocID, d.TrCode, d.AccountType
	FROM AccDocument d
	WHERE d.TrSign = 'C'
		AND d.AccountType = 'AR'
	UNION
	SELECT DISTINCT d.TrDocID, d.TrCode, t.AccountType
	FROM AccDocument d
		INNER JOIN AccTransfer t
