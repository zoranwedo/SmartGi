	WHERE d.TrSign = 'C'
		AND d.AccountType != 'AR'
		AND t.CreditType = 'AR'
) q LEFT OUTER JOIN AccDocProforma AS p
	ON p.TrDocID = q.TrDocID
WHERE p.TrDocID IS NULL
ORDER BY 2</CommandText>
