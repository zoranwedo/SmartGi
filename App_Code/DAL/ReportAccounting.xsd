                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT j.HotelId,
       j.TourOpId,
       j.DocumentID,
       j.TransDate,
       j.DocNumber,
       j.DocOrd,
       j.Reference,
       j.Amount,
       j.HAmount,
