       j.DocCode,
       j.TrDescription,
       j.Note,
       AccTransDocument.DepID,
       AccTransDocument.EmpID,
       Employee.EmpName,
       Department.DepName,
       j.PayModeID
FROM   Invoice
       RIGHT OUTER JOIN dbo.AccRepDocJournal(
                @HotelID,
                @HotelGroupID,
                @TourOpID,
                @TourOpGroupID,
                @MarketID,
                @PayCondID,
                @DateFrom,
                @DateTo,
                @Canceled,
                @TrDocClass,
                @TrClass,
                @TrCode,
                @AccTypeID,
                @TranTypeID,
                @DocTypeID,
                @TrSign,
                @InvType
            ) AS j
            ON  Invoice.InvNumber = j.DocNumber
       LEFT OUTER JOIN Employee
       INNER JOIN Department
            ON  Employee.DepID = Department.DepID
       RIGHT OUTER JOIN AccTransDocument
            ON  Employee.DepID = AccTransDocument.DepID
            AND Employee.EmpID = AccTransDocument.EmpID
            ON  j.DocumentID = AccTransDocument.DocumentID
WHERE  (@InvType IS NULL)
       OR  (Invoice.InvType = @InvType)
ORDER BY
       j.Hotel,
       j.TourOp,
       j.TrDocClass,
       j.TrClass,
