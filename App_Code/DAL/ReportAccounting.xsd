              <DbSource ConnectionRef="csSMART_SQL (Web.config)" DbObjectType="Unknown" GenerateMethods="Get" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="GetData">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     d.TransID, d.TransDate, d.TransNote, d.Canceled, AccTransDetail_1.TransOrd, AccTransDetail_1.HotelId, AccTransDetail_1.TourOpId, 
                      AccTransDetail_1.TrCode, AccTransDetail_1.TrAccount, AccTransDetail_1.DebitAmount, AccTransDetail_1.DebitHAmount, 
                      AccTransDetail_1.CreditHAmount, AccTransDetail_1.CreditAmount, AccTransDetail_1.UserName, AccTransDetail_1.DocumentID, 
                      AccTransDocument.DocNumber, AccTransDocument.DocCode, AccTransDocument.Reference, AccTransDocument.Note, AccTransfer.Description, 
                      AccTransfer.CreditType, AccTransfer.DebitType, AccTransfer.TrSign, AccTransfer.AccountType, AccTransfer.TrClass
FROM         AccTransfer INNER JOIN
                      AccTransaction AS AccTransaction_1 ON AccTransfer.TransferID = AccTransaction_1.TransferID RIGHT OUTER JOIN
                          (SELECT DISTINCT 
                                                   TOP (100) PERCENT AccTransaction.TransID, AccTransaction.TransDate, AccTransaction.TransNote, AccTransaction.Canceled
                            FROM          AccTransaction INNER JOIN
                                                   AccTransDetail ON AccTransaction.TransID = AccTransDetail.TransID
                            WHERE      (AccTransDetail.DocumentID = @DocumentID) AND (@ShowCancel = 1) OR
                                                   (AccTransDetail.DocumentID = @DocumentID) AND (AccTransDetail.Canceled = 0)
                            ORDER BY AccTransaction.TransDate, AccTransaction.TransID) AS d INNER JOIN
                      AccTransDetail AS AccTransDetail_1 ON d.TransID = AccTransDetail_1.TransID ON 
                      AccTransaction_1.TransID = AccTransDetail_1.TransID LEFT OUTER JOIN
                      AccTransDocument ON AccTransDetail_1.DocumentID = AccTransDocument.DocumentID</CommandText>
