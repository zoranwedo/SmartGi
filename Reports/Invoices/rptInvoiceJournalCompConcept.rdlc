      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>	SELECT
		q.InvoiceID, 
		q.InvNumber, 
		q.InvOrder, 
		q.HotelId, 
		q.Hotel, 
		q.SortPriority,
		q.TourOpId, 
		q.BillingName,
		q.TourOp,
		q.InvDoc, 
		q.InvType, 
		q.ResGroup, 
		q.InvDate,
		q.CheckIn, 
		q.CheckOut, 
		q.Amount,	 
		q.RefText,
		q.InvRefText,
		q.[State],
		q.InvPrnText, 
		q.InvoiceState,
		q.HOAmount,
		q.CCIReview,		
		q.OAmount,
		q.InvComment,
		q.MarketId, 
		q.PayCondId,
		q.CommentDate, 
		q.CommentUser, 
		q.[Comment],
		q.ConceptId, 
		q.ConceptCode, 
		q.ConceptName, 
		q.ConceptShName, 
		q.ConceptAmount,
		q.ConceptSum,
		q.RNK
	FROM dbo.[Invoice_Prev_GRP_Concept](
			@HRate,
			@InvDoc,
			@HotelGroupID,
			@HotelID,
			@TourOpGroupID,
			@TourOpId,
			@PayCondId,
			@MarketId,
			@InvType,
			@InvNumFrom,
			@InvNumTo,
			@ChkInFrom,
			@ChkInTo,
			@InvDateFrom,
			@InvDateTo,
			@State,
			@StateSet,
			@IncludeCancel
	) q
</CommandText>
