      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT 
	j.HotelId, j.Hotel, j.SortPriority, 
	j.TourOpId, j.BillingName, j.TourOp, j.MarketId, j.PayCondID, 
	j.InvoiceID, j.InvNumber, j.InvOrder, j.InvDoc, j.InvType, 
	j.ResGroup, j.ResGroupName, j.InvDate, 
	j.CheckIn, j.CheckOut,
	j.Amount, j.cAmount, j.hAmount, 
	j.InvRefText, j.InvPrnText, j.InvComment, 
	j.[State], j.InvoiceState, j.CCIReview, 
	j.DeliveryDate, j.Delivered,
	ir.cmPercent, 
	CASE
		WHEN @HRate = 1 THEN ir.cmHRebate
		ELSE ir.cmRebate
	END cmRebate 
FROM dbo.InvoiceJournal (
	@HRate,
	@InvDoc,
	@HotelID,
	@TourOpID,
	@PayCondID,
	@MarketID,
	@InvType,
	@GroupCode,
	@InvNumFrom,
	@InvNumTo,
	@ChkInFrom,
	@ChkInTo,
	@InvDateFrom,
	@InvDateTo,
	@State,
	@StateSet,
	@IncludeCancel) j
	   LEFT OUTER JOIN InvRebateSum ir
			ON ir.InvoiceID = j.InvoiceID 
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
