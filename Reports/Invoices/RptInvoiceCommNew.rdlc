      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT        Invoice_Print_1.InvoiceID, Invoice_Print_1.InvNumber, Invoice_Print_1.InvOrder, Invoice_Print_1.HotelId, Invoice_Print_1.Hotel, Invoice_Print_1.TourOpId, 
                         Invoice_Print_1.BillingName, Invoice_Print_1.TourOp, Invoice_Print_1.InvDoc, Invoice_Print_1.InvType, Invoice_Print_1.ResGroup, Invoice_Print_1.InvDate, 
                         Invoice_Print_1.CheckIn, Invoice_Print_1.CheckOut, Invoice_Print_1.Amount, Invoice_Print_1.RefText, Invoice_Print_1.InvRefText, Invoice_Print_1.InvPrnText, 
                         Invoice_Print_1.State, Invoice_Print_1.InvoiceState, Hotel.RNC, Hotel.Address, Hotel.City, Hotel.Tel, Hotel.Fax, Hotel.State AS Expr1, TourOp.Address AS AddressTO, 
                         TourOp.City AS CityTO, TourOp.State AS StateTO, TourOp.Country AS CountryTO, TourOp.ZipCode AS ZipCodeTO, HotelCCI.Hotel AS CCIHotel, 
                         HotelCCI.RNC AS CCIRNC, HotelCCI.Address AS CCIAddress, HotelCCI.City AS CCICity, HotelCCI.ZipCode AS CCIZipCode, HotelCCI.Country AS CCICountry, 
                         HotelCCI.State AS CCIState, HotelCCI.Tel AS CCITel, HotelCCI.Fax AS CCIFax
FROM            dbo.Invoice_Print(@InvoiceID, @HRate) AS Invoice_Print_1 INNER JOIN
                         Hotel ON Invoice_Print_1.HotelId = Hotel.HotelID LEFT OUTER JOIN
                         TourOp ON Invoice_Print_1.TourOpId = TourOp.TourOpID CROSS JOIN
                         Hotel AS HotelCCI
WHERE        (HotelCCI.HotelID = 0)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
