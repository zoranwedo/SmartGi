      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT ipg.InvoiceID,
       ipg.InvNumber,
       ipg.InvOrder,
       ipg.HotelId,
       ipg.Hotel,
       ipg.SortPriority,
       ipg.TourOpId,
       ipg.BillingName,
       ipg.TourOp,
       ipg.InvDoc,
       ipg.InvType,
       ipg.ResGroup,
       ipg.InvDate,
       ipg.CheckIn,
       ipg.CheckOut,
       ISNULL(ipg.Amount, 0)    AS Amount,
       ipg.RefText,
       ipg.InvRefText,
       ipg.State,
       ipg.InvPrnText,
       ipg.InvoiceState,
       ISNULL(ipg.HOAmount, 0)  AS HOAmount,
       ipg.CCIReview,
       ISNULL(ipg.OAmount, 0)   AS OAmount,
       Invoice.InvComment,
       t.MarketId, 
       t.PayCondId,
       c.CommentDate, 
       c.CommentUser, 
       c.InvComment				AS [Comment]
FROM   dbo.Invoice_Prev(
           @HotelID,
           @TourOpId,
           @HRate,
           @InvNumFrom,
           @InvNumTo,
           @ChkInFrom,
           @ChkInTo,
           @InvDateFrom,
           @InvDateTo,
           @InvDoc,
           @InvType,
           @State,
           @IncludeCancel,
           @CCIReview
       )                        AS ipg
       INNER JOIN Invoice
            ON  ipg.InvoiceID = Invoice.InvoiceID
       INNER JOIN TourOp t
	        ON t.TourOpID = Invoice.TourOpId
	   left outer JOIN InvoiceComment c
		ON ipg.InvoiceID = c.InvoiceID
WHERE  ((@MarketId IS NULL) OR (t.MarketId = @MarketId))
       AND ((@PayCondId IS NULL) OR (t.PayCondId = @PayCondId))
ORDER BY
       ipg.SortPriority,
       ipg.TourOp,
       ipg.InvDate,
       ipg.InvNumber</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
