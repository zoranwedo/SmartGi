      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT     s.SpecialID, s.HotelId, h.Hotel, s.Category, s.BookingCode, s.RateOption, 
                      CASE s.RateOption WHEN 0 THEN 'PerArrival' WHEN 1 THEN 'PerStay' ELSE 'Inclusiv' END AS RateOptionDisp, s.Start, s.Finish, s.BkStart, 
                      s.BkFinish, s.Payment, s.RoomingList, s.StayCriteriaMin, s.StayCriteriaMax, s.PrDiscountTypeId, d1.DiscountType AS PrType, s.PrDiscountOn, 
                      CASE s.PrDiscountOn WHEN 'F' THEN 'All' WHEN 'A' THEN 'Adults' WHEN 'E' THEN 'ExtraPax' WHEN 'C' THEN 'Children' ELSE '-' END AS PrOn, 
                      s.PrDiscount, s.PrPropagate, s.ScDiscountTypeId, ISNULL(d2.DiscountType, '-') AS ScType, s.ScDiscountOn, 
                      CASE s.ScDiscountOn WHEN 'F' THEN 'All' WHEN 'A' THEN 'Adults' WHEN 'E' THEN 'ExtraPax' WHEN 'C' THEN 'Children' ELSE '-' END AS ScOn, 
                      s.ScDiscount, s.Description, s.StateId, s.CtrEBB, s.RateTypeId, s.RateCodeId, s.Title, s.RateRecieved
FROM         Special AS s INNER JOIN
                      Hotel AS h ON s.HotelId = h.HotelID LEFT OUTER JOIN
                      SpecialDiscountType AS d1 ON s.PrDiscountTypeId = d1.DiscountTypeID LEFT OUTER JOIN
                      SpecialDiscountType AS d2 ON s.ScDiscountTypeId = d2.DiscountTypeID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
