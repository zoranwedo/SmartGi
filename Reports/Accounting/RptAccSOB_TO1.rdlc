      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT     AccRepSOB_1.HotelId, AccRepSOB_1.TourOpId, AccRepSOB_1.Rooms, AccRepSOB_1.ChldNum, AccRepSOB_1.Pax, AccRepSOB_1.Revenue, 
                      AccRepSOB_1.HRevenue, Hotel.Hotel, TourOp.TourOp, TourOp.MarketId, CASE AccRepSOB_1.Rooms WHEN NULL 
                      THEN 0 ELSE (Revenue / AccRepSOB_1.Rooms) END AS RoomRate, CASE AccRepSOB_1.Rooms WHEN NULL 
                      THEN 0 ELSE (HRevenue / AccRepSOB_1.Rooms) END AS HRoomRate, CASE Pax WHEN NULL THEN 0 ELSE Revenue / Pax END AS PaxRate, 
                      CASE Pax WHEN NULL THEN 0 ELSE HRevenue / Pax END AS HPaxRate
FROM         dbo.AccRepSOB(@HotelID, @HotelGroupID, @TourOpID, @TourOpGroupID, @MarketID, @PayCondID, @DateFrom, @DateTo) 
                      AS AccRepSOB_1 INNER JOIN
                      Hotel ON AccRepSOB_1.HotelId = Hotel.HotelID INNER JOIN
                      TourOp ON AccRepSOB_1.TourOpId = TourOp.TourOpID</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
