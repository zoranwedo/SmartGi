      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT HotelId, 
        TourOpId, DocumentID, DepID, EmpID, DepName, EmpName, 
        Hotel, TourOp, MarketId, CreditDays, TrDocClass, TrClass, TrAccount, TrSign, 
        TrCode, TrNumber, TrOrd, TransID, TransOrd, TransDate, 
        AppDocID TrAppDocID,
        AppDocID, AppCode, AppNumber, AppOrd, AppAccount, AppRef, 
        DebitAmount, DebitHAmount, CreditAmount, CreditHAmount,
        ReservedAmount, ReservedHAmount
FROM dbo.AccRepDocBalanceDetail_new(@HotelID, @HotelGroupID, @TourOpID, @TourOpGroupID, 
        @MarketID, @PayCondID, @AsOffDate, @DocFrom, @TrDocClass, @TrClass, @TrCode, 
        @AccTypeID, @TranTypeID, @DocTypeID, @TrSign, @RptOption, @DepID, @EmpID) AS q
WHERE @Zero = 1 OR 
        q.TransID = 0 OR 
        COALESCE(q.DebitAmount,q.CreditAmount) &gt; 0
--ORDER BY Hotel, TourOp, TrDocClass, TrClass, TrCode, TrNumber, TransDate, TransID, TransOrd

</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
