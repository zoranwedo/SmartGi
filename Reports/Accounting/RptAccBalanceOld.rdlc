        <CommandText>SELECT     HotelId, TourOpId, DocumentID, Hotel, TourOp, MarketId, CreditDays, TrDocClass, TrClass, TrAccount, TrSign, TrCode, TrNumber, TrOrd, TransID, 
                      TransOrd, TransDate, AppDocID, AppCode, AppNumber, AppOrd, AppAccount, AppRef, DebitAmount, DebitHAmount, CreditAmount, CreditHAmount
FROM         dbo.AccRepDocBalanceDetail(@HotelID, @HotelGroupID, @TourOpID, @TourOpGroupID, @MarketID, @PayCondID, @AsOffDate, @DocFrom, 
                      @TrDocClass, @TrClass, @TrCode, @AccTypeID, @TranTypeID, @DocTypeID, @TrSign) AS AccRepDocBalanceDetail_1
