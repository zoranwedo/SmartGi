	q.DepID,
	q.EmpID,
	q.DepName,
	q.EmpName,
	q.AccResAmount,
	q.AccResHAmount
FROM AccRepDocBalance_new(
	@HotelID, @HotelGroupID,
	@TourOpID, @TourOpGroupID, @MarketID, @PayCondID,
	@AsOffDate, @DocFrom,
	@TrDocClass, @TrClass, @TrCode, @AccTypeID, 
	@TranTypeID, @DocTypeID, @TrSign,
	@RptOption, @DepID, @EmpID) q
	</CommandText>
