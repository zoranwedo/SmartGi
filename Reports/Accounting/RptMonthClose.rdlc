      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT q.HotelId,
       q.Hotel,
       q.TrDocClass,
       q.TrTypeClass,
       q.TrClass,
       q.TrCode,
       q.TrAccount,
       q.AccountTypeID,
       q.AccountType,
       q.TranTypeID,
       q.TranType,
       q.DocTypeID,
       q.DocType,
       q.TrSign,
       c.TrDescription AS TrClassDesc,
       q.TrDescription,
       SUM(q.CreditBalance) CreditTotal,
       SUM(q.CreditHBalance) CreditHTotal,
       SUM(q.DebitBalance) DebitTotal,
       SUM(q.DebitHBalance) DebitHTotal,
       q.MSort
FROM   (
           SELECT *
           FROM   [LIVE_SMART_SQL].[dbo].[AccRepMonthly] (
                      @HotelID,
                      @HotelGroupID,
                      @TourOpID,
                      @TourOpGroupID,
                      @MarketID,
                      @PayCondID,
                      @AsOffDate,
                      @PrDate,
                      @AccTypeID
                  )
       ) q
       INNER JOIN AccDocClass c
			ON c.TrClass = q.TrClass
GROUP BY
       q.HotelId,
       q.Hotel,
       q.TrDocClass,
       q.TrTypeClass,
       q.TrClass,
       q.TrCode,
       q.TrAccount,
       q.AccountTypeID,
       q.AccountType,
       q.TranTypeID,
       q.TranType,
       q.DocTypeID,
       q.DocType,
       q.TrSign,
       c.TrDescription,
       q.TrDescription,
       q.MSort
ORDER BY
       q.HotelId,
       q.Hotel,
       q.MSort,
       q.TrDocClass,
       q.TrTypeClass,
       q.TrClass,
       q.TrCode,
       q.TrAccount,
       q.AccountTypeID,
       q.AccountType,
       q.TranTypeID,
       q.TranType,
       q.DocTypeID,
       q.DocType,
       q.TrSign,
       q.TrDescription

</CommandText>
