      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT 
	st.HotelId, 
	st.TourOpId, 
	st.Hotel, 
	st.HotelSort, 
	st.TourOp, 
	st.MarketId, 
	st.CreditDays,
	st.TransID, 
	st.TransDate, 
	st.DebitDocID, 
	st.DebitAccount, 
	st.DebitCode,
	st.DebitNumber, 
	st.DebitOrd, 
	ISNULL(st.DebitAmount,0) DebitAmount,
	st.Note,
	st.CreditDocID, 
	st.CreditCode, 
	st.CreditNumber, 
	st.CreditOrd, 
	ISNULL(st.CreditAmount,0) CreditAmount,
	st.Reference, 
	st.InvoiceID, 
	st.CheckIn, 
	st.CheckOut,
	st.SortField, 
	st.TrDocClass, 
	st.TrClass, 
	st.AccountType, 
	st.AccountTypeID,
	st.AppComment,
	CONVERT(MONEY,NULL) arBalance,
	CONVERT(MONEY,NULL) flBalance,
	CONVERT(MONEY,NULL) ccBalance,
	CONVERT(MONEY,NULL) mcBalance,
	CONVERT(MONEY,NULL) icBalance
FROM dbo.AccStatement(
		@HotelID, @HotelGroupID, 
		@TourOpID, @TourOpGroupID, @MarketID, @PayCondID, 
		@AsOffDate, 
		@AccType, @InvType,
		@BalanceStatus) st
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
