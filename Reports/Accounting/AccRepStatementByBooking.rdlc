      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT 
	a.HotelId, 
	a.TourOpId, 
	a.Hotel, 
	a.HotelSort, 
	a.TourOp, 
	a.MarketId,
	a.CreditDays,
	a.TransID,
	a.TransDate, 
	a.DebitDocID,
	a.DebitAccount, 
	a.DebitCode, 
	a.DebitNumber, 
	a.DebitOrd, 
	a.DebitAmount,
	a.Note, 
	a.CreditDocID, 
	a.CreditCode, 
	a.CreditNumber,
	a.CreditOrd, 
	a.CreditAmount, 
	a.Reference, 
	a.CheckIn,
	a.CheckOut, 
	a.SortField, 
	a.TrDocClass,
	a.TrClass,
	a.AccountType,
	a.AccountTypeID,
	a.AppComment, 
	a.FolioFull, 
	a.DetailID, 
	a.DetText,
	a.BookingVoucher, 
	a.StayAmount, 
	a.PayAmount, 
	a.Automatic
FROM dbo.AccStatementBooking(
	@HotelID, @HotelGroupID, 
	@TourOpID, @TourOpGroupID, @MarketID, @PayCondID, 
	@AsOffDate, @AccType, 
	@InvType, @BalanceStatus) a</CommandText>
