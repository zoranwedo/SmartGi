      <Query>
        <DataSourceName>csSMART_SQL</DataSourceName>
        <CommandText>SELECT 
	i.InvoiceID, 
	i.InvNumber, 
	i.InvOrder, 
	i.InvDate, 
	i.InvDoc, 
	i.InvType,
	i.ResGroup, 
    i.HotelId, 
    i.TourOpId, 
    i.BillingName, 
    i.CheckIn, 
    i.CheckOut,
	i.Nights, 
	i.Amount, 
	i.HAmount, 
	i.ITBIS, 
	i.InvRefText, 
	i.InvPrnText,
	i.InvComment, 
	i.[State], 
	i.CreateUser, 
	i.CreateDate, 
	i.EditUser, 
	i.EditDate,
	i.DeliveryDate, 
	i.HOAmount, 
	i.CCIReview, 
	i.OAmount, 
	i.Hotel, 
	i.TourOp,
	ISNULL(i.InvSource,'SH') InvSource,
	CASE i.State
		WHEN 1 THEN 'Not Reviewed by Hotel'
		WHEN 2 THEN 'Reviewed by Hotel'
		WHEN - 3 THEN 'Canceled by Hotel'
		WHEN 4 THEN 'Sent from hotel'
		WHEN 5 THEN 'Reviewed by CCIl'
		WHEN 6 THEN 'Modified by CCI'
		ELSE 'Canceled by CCI'
	END StateDisp,
	CASE i.InvType
		WHEN 0 THEN 'Stay Inv'
		WHEN 1 THEN 'Group Inv'
		WHEN 2 THEN 'Cancel Inv'
		ELSE 'No-Show Inv'
	END TypeDisp
FROM [dbo].[FilterUserInvoiceNew] (
  @UserName,
  @HotelID,
  @HotelGroupID,
  @TourOpID,
  @TourOpGroupID,
  @MarketID,
  @PayCondID,
  @InvDoc,
  @InvType,
  @InvNumFrom,
  @InvNumTo,
  @ChkInFrom,
  @ChkInTo,
  @BkNumber,
  @State,
  @StateSet,
  @IncludeCancel,
  @IncludeDelivered) i
ORDER BY 
	i.Hotel, 
	i.CheckIn, 
	i.TourOp, 
	i.CheckOut, 
	i.InvNumber</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
